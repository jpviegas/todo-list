{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin: import/errors",
    "plugin: jsx-a11y/recommended",
    "plugin: react-hooks/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "import", "jsx-a11y"],
  "rules": { "react/prop-types": 0, "react/react-in-jsx-scope": 0 }
}
